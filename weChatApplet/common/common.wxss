/* 布局相关 */
.border-box{
  box-sizing: border-box;
}
.none {
  display: none;
}
.inline-block {
  display: inline-block;
}
.block {
  display: block;
}
.table {
  display: table
}
.table-cell {
  display: table-cell
}
.vertical-align-middle {
  vertical-align: middle;
}
/* 图片 */
.img{
  width: 100%;
}
/*清除浮动*/
.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
/*左浮动，右浮动*/
.pull-left {
  float: left;
}
.pull-right {
  float: right;
}
.absolute {
  position: absolute;
}
.relative {
  position: relative;
}
.fixed {
  position: fixed;
}
/* 文字 */
.fontBold {
  font-weight: bold;
}
.bold {
  font-weight: bold;
}
.text-center {
  text-align: center;
}
.text-left {
  text-align: left
}
.text-right {
  text-align: right
}
/* 颜色 */
.gray {
  color: #797979;
}
.orange-price{
  color: #FBA500;
}